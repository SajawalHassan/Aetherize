generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserTier {
  FREE
  STARTER
  PRO
}

model User {
  id       String   @id @default(uuid())
  name     String
  email    String   @unique
  imageUrl String?
  password String?
  tier     UserTier
}
